    "@tinacms/datalayer": "^1.2.39",
    "next": "^14.2.4",
    "next-auth": "^4.24.7",
    "rc-slider": "^10.0.0",
    "react": "^18.3.1",
    "react-countup": "^6.2.0",
    "react-dom": "^18.3.1",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^9.1.2",
    "react-visibility-sensor": "^5.1.1",
    "swiper": "^8.4.4",
    "tinacms": "^2.2.8",
    "tinacms-gitprovider-github": "^1.0.10",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.3",
    "@tinacms/cli": "^1.6.8",
    "eslint": "8.13.0",
    "eslint-config-next": "^14.2.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
